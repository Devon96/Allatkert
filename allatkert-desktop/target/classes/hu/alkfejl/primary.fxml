<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hu.alkfejl.PrimaryController">
   <children>
      <MenuBar prefHeight="26.0" prefWidth="600.0">
        <menus>
          <Menu mnemonicParsing="false" text="Listázás">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#switchToListAllat" text="Állatok listázása" />
              <MenuItem mnemonicParsing="false" onAction="#switchToListOrokbefogado" text="Örökbefogadók listázása" />
              <MenuItem mnemonicParsing="false" onAction="#switchToListKonyveles" text="Könyvelések listázása" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Beszúrás">
            <items>
              <MenuItem mnemonicParsing="false" text="Új állat hozzáadása" onAction="#addAllat"/>
              <MenuItem mnemonicParsing="false" text="Új örökbefogadó hozzáadása" onAction="#addOrokbefogado" />
              <MenuItem mnemonicParsing="false" text="Új könyvelés hozzáadása" onAction="#addKonyveles" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </children>
</AnchorPane>
